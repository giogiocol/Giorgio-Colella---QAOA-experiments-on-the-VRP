    "def cost_func_estimator_job(params, ansatz, hamiltonian, estimator):\n",
    "    global min_cost, opt_params, cost_history, params_history\n",
    "    # transform the observable defined on virtual qubits to\n",
    "    # an observable defined on all physical qubits\n",
    "    isa_hamiltonian = hamiltonian.apply_layout(ansatz.layout)\n",
    " \n",
    "    pub = (ansatz, isa_hamiltonian, params)\n",
    "    job = estimator.run([pub])\n",
    "    print('waiting for job to complete...')\n",
    "    job.wait_for_final_state()   # waits until job finishes\n",
    " \n",
    "    results = job.result()[0]\n",
    "    cost = results.data.evs\n",
    "\n",
    "    if cost < min_cost:\n",
    "        min_cost = cost\n",
    "        opt_params = params\n",
    "\n",
    "    cost_history.append(cost)\n",
    "    params_history.append(np.array(params, dtype=float))\n",
    "\n",
    "    print(f'--> {params_history}')\n",
    "\n",
    "    return cost"
   ]
